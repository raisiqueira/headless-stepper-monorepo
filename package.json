{
  "name": "headless-stepper-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run headless-stepper:storybook",
    "dev:docs": "cd docs && npm run dev",
    "prebuild": "cp -r ./README.md ./packages/headless-stepper",
    "build:lib": "npm run prebuild && nx build headless-stepper",
    "nx": "nx",
    "np": "np",
    "publish:prod": "npm run build:lib && cd ./dist/packages/headless-stepper && npm publish",
    "publish:dev": "npm run build:lib && cd ./dist/packages/headless-stepper && npm publish --tag next",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "test": "npm run nx run headless-stepper:test",
    "test:watch": "npm run nx run headless-stepper:test -- --watch",
    "postinstall": "(cd docs && npm install)"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "7.12.13",
