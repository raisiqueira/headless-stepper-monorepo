{
  "name": "headless-stepper-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run headless-stepper:storybook",
    "dev:docs": "cd docs && npm run dev",
    "prebuild": "cp -r ./README.md ./packages/headless-stepper",
    "build:lib-fast": "npm run prebuild && pnpm exec tsup --config packages/headless-stepper/tsup.config.ts",
    "build:lib": "npm run prebuild && pnpm exec tsup --config packages/headless-stepper/tsup.config.ts --dts-resolve",
    "build:components": "pnpm exec tsup --config packages/headless-stepper/components/tsup.config.ts --dts-resolve",
    "build:components-fast": "pnpm exec tsup --config packages/headless-stepper/components/tsup.config.ts",
    "build:all": "pnpm run build:lib && pnpm run build:components",
    "nx": "nx",
    "publish:prod": "npm run build:all && cd ./packages/headless-stepper && npm publish",
    "publish:dev": "npm run build:all && cd ./packages/headless-stepper && npm publish --tag next",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "test": "nx test headless-stepper",
    "test:watch": "npm run nx run headless-stepper:test -- --watch"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "7.12.13",
    "@nrwl/rollup": "16.4.2",
    "@nx/cypress": "16.4.2",
    "@nx/eslint-plugin": "16.4.2",
    "@nx/js": "16.4.2",
    "@nx/linter": "16.4.2",
    "@nx/react": "16.4.2",
    "@nx/storybook": "16.4.2",
    "@nx/vite": "16.4.2",
    "@nx/web": "16.4.2",
    "@nx/workspace": "16.4.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@size-limit/preset-big-lib": "^7.0.8",
    "@storybook/addon-essentials": "7.0.24",
    "@storybook/core-server": "7.0.24",
    "@storybook/react": "7.0.24",
    "@storybook/react-webpack5": "7.0.24",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/node": "18.11.9",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "autoprefixer": "10.4.13",
    "babel-loader": "8.1.0",
    "babel-plugin-optimize-clsx": "^2.6.2",
    "css-loader": "^6.4.0",
    "cypress": "^9.1.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jsdom": "22.1.0",
    "next-transpile-modules": "^9.0.0",
    "nx": "16.4.2",
    "nx-cloud": "16.0.5",
    "postcss": "8.4.19",
    "prettier": "2.7.1",
    "react-refresh": "^0.10.0",
    "size-limit": "^7.0.8",
    "storybook": "7.0.24",
    "style-loader": "^3.3.0",
    "stylus": "0.59.0",
    "stylus-loader": "^7.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "5.1.6",
    "url-loader": "^4.1.1",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.4",
    "webpack": "^5.75.0",
    "webpack-merge": "^5.8.0"
  },
  "workspaces": [
    "packages/**"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "next": "^13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "^2.0.0"
  },
  "size-limit": [
    {
      "path": "packages/headless-stepper/dist/index.js",
      "limit": "15 KB"
    },
    {
      "path": "packages/headless-stepper/dist/index.mjs",
      "limit": "15 KB"
    }
  ]
}
